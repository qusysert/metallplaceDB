                    "format": "bool",
                    "example": true
                }
            }
        },
        "handler.AvgPriceRequest": {
            "type": "object",
            "properties": {
                "finish": {
                    "type": "string",
                    "format": "string",
                    "example": "2020-01-01"
                },
                "material_source_id": {
                    "type": "integer",
                    "format": "int64",
                    "example": 2
                },
                "property_id": {
                    "type": "integer",
                    "format": "int64",
                    "example": 1
                },
                "start": {
                    "type": "string",
                    "format": "string",
                    "example": "2019-01-01"
                }
            }
        },
        "handler.AvgPriceResponse": {
            "type": "object",

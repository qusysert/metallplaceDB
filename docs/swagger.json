        "handler.AddValueRequest": {
            "type": "object",
            "properties": {
                "created_on": {
                    "type": "string",
                    "format": "string",
                    "example": "2029-09-14"
                },
                "material_source_id": {
                    "type": "integer",
                    "format": "int64",
                    "example": 2
                },
                "property_name": {
                    "type": "string",
                    "format": "string",
                    "example": "Средняя цена"
                },
                "value_float": {
                    "type": "string",
                    "format": "string",
